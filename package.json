{
  "name": "@backtrack/core",
  "version": "0.0.3",
  "main": "dist/backtrack.js",
  "license": "ISC",
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=3.10.10",
    "yarn": ">=1.3.2"
  },
  "devEngines": {
    "node": ">=8.9.0",
    "npm": ">=5.5.1"
  },
  "bin": {
    "backtrack": "./dist/cli/cli.js"
  },
  "scripts": {
    "clean": "del-cli ./dist/*",
    "lint": "eslint .",
    "lint.fix": "eslint --fix .",
    "flow": "flow",
    "format": "prettier */**/*.{js,md,json} --write",
    "test": "jest",
    "build": "npm run clean && concurrently \"npm run build.babel\" \"npm run build.flow\"",
    "build.babel": "cross-env NODE_ENV=production babel src -d dist --ignore test.js,__sandbox__/ --source-maps",
    "build.flow": "cross-env NODE_ENV=production flow-copy-source --ignore \"**/*.test.js\" --ignore \"**/__sandbox__/**/*\" src dist",
    "dev": "npm run clean && concurrently \"npm run dev.babel\" \"npm run dev.flow\"",
    "dev.babel": "cross-env NODE_ENV=development babel -w src/ -d dist/ --ignore test.js,__sandbox__/ --source-maps",
    "dev.flow": "cross-env NODE_ENV=development flow-copy-source -w --ignore \"**/*.test.js\" --ignore \"**/__sandbox__/**/*\" src dist"
  },
  "lint-staged": {
    "*.{js,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@chrisblossom/eslint-config": "^2.0.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "del-cli": "^1.1.0",
    "eslint": "^4.19.1",
    "flow-bin": "^0.69.0",
    "flow-copy-source": "^1.3.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-serializer-path": "^0.1.13",
    "jest-snapshot-serializer-function-name": "^1.1.0",
    "lint-staged": "^7.0.4",
    "prettier": "^1.11.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.3.2",
    "cosmiconfig": "^4.0.0",
    "deep-keys": "^0.5.0",
    "del": "^3.0.0",
    "dotenv": "^5.0.1",
    "ex-config": "^1.0.5",
    "execa": "^0.10.0",
    "fs-extra": "^5.0.0",
    "joi": "^12.0.0",
    "lodash": "^4.17.5",
    "parent-module": "^0.1.0",
    "read-pkg-up": "^3.0.0",
    "resolve-with-prefix": "^1.0.3",
    "slash": "^2.0.0",
    "sort-keys": "^2.0.0",
    "source-map-support": "^0.5.4"
  }
}
